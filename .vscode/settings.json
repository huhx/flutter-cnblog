{
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true,
        ".vscode": true,
        ".idea": true,
        ".dart_tool": true,
        "build": true,
        "flutter_cnblog.iml": true,
        ".flutter-plugins": true,
        ".flutter-plugins-dependencies": true,
        "symbols": true,
        "**/*.g.dart": true,
        "android/.gradle": true,
        "ios/.symlinks": true,
        "ios/Pods": true,
    },
    "editor.minimap.enabled": false,
    "editor.fontFamily": "Monaco, 'Courier New', monospace",
    "editor.fontSize": 12,
    "workbench.colorTheme": "JetBrains Darcula Theme",
    "gitlens.mode.active": "zen",
    "dart.closingLabels": false,
    "dart.lineLength": 120,
    "editor.rulers": [120],
    "workbench.colorCustomizations": {
        "list.hoverBackground": "#44475A75",
        "list.focusBackground": "#b975c2"    
    }
}